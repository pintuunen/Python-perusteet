luku1 = input("Anna ensimmÃ¤inen luku: ")
luku1 = int(luku1)
luku2 = input("Anna toinen luku: ")
luku2 = int(luku2)
print("""(1) +
(2) -
(3) *
(4) /""")
valinta = input("Tee valinta (1-4): ")
valinta =int(valinta)
if (valinta==1):
    print("Tulos on: ", luku1+luku2)
elif (valinta == 2):
    print("Tulos on: ", luku1-luku2)
elif (valinta == 3):
    print("Tulos on: ", luku1 * luku2)
elif (valinta == 4):
    print("Tulos on: ", luku1 / luku2)
else:
    print("Valintaa ei tunnistettu.")
